{
  "name": "claude-code-data",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "TypeScript library for parsing and analyzing Claude Code conversation data",
  "keywords": [
    "claude-code",
    "conversation",
    "parser",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/claude-code-data.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "([ -n \"$CI\" ] || build-ts-on-mbp) && tsc --project tsconfig.build.json",
    "preview": "vite preview",
    "test": "npm run typecheck && vitest run",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@vitest/ui": "^3.2.1",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.1"
  }
}
