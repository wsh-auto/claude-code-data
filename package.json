{
  "name": "claude-code-data",
  "packageManager": "bun@1.1.45",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "TypeScript library for parsing and analyzing Claude Code conversation data",
  "keywords": [
    "claude-code",
    "conversation",
    "parser",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/claude-code-data.git"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test": "bun run lint && bun run typecheck && timeout --foreground 300 vitest run",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint '{src,test}/**/*.ts'",
    "lint:fix": "eslint '{src,test}/**/*.ts' --fix"
  },
  "dependencies": {
    "@mdr/lib-utils": "github:wsh-auto/lib-utils"
  },
  "optionalDependencies": {
    "@mdr/lib-log": "link:@mdr/lib-log"
  },
  "devDependencies": {
    "@eslint/js": "8.57.0",
    "@mdr/dev-typescript": "link:@mdr/dev-typescript",
    "@types/node": "^22.15.29",
    "@vitest/ui": "^3.2.1",
    "eslint": "8.57.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "7.11.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.1"
  }
}
